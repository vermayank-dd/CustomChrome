{
  "manifest_version": 3,
  "name": "Custom New Tab",
  "version": "1.0",
  "description": "A beautiful new tab page with text editor and todo list",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage",
    "topSites"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 